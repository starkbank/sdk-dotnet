<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <ReleaseVersion>0.0.1</ReleaseVersion>
    <PackOnBuild>true</PackOnBuild>
    <PackageVersion>0.0.1</PackageVersion>
    <NeutralLanguage>en-US</NeutralLanguage>
    <Owners>StarkBank</Owners>
    <PackageProjectUrl>https://github.com/starkbank/sdk-dotnet</PackageProjectUrl>
    <Summary>SDK to facilitate C# (.NET) integrations with the Stark Bank API</Summary>
    <PackageTags>starkbank, openbanking, open, banking, bank</PackageTags>
    <Title>StarkBank SDK</Title>
    <Description>SDK to facilitate C# (.NET) integrations with the Stark Bank API</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="starkbank-ecdsa" Version="1.0.0" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Utils\" />
    <Folder Include="User\" />
    <Folder Include="Ledger\" />
    <Folder Include="Boleto\" />
    <Folder Include="Transfer\" />
    <Folder Include="Payment\" />
    <Folder Include="Payment\Boleto\" />
    <Folder Include="Payment\Utility\" />
    <Folder Include="Webhook\" />
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="Utils\Environment.cs" />
  </ItemGroup>
</Project>
