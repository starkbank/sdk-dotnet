<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
    <ReleaseVersion>2.5.0</ReleaseVersion>
    <PackageVersion>2.5.0</PackageVersion>
    <NeutralLanguage>en-US</NeutralLanguage>
    <Owners>Stark Bank</Owners>
    <PackageProjectUrl>https://github.com/starkbank/sdk-dotnet</PackageProjectUrl>
    <Summary>SDK to facilitate C# (.NET) integrations with the Stark Bank API</Summary>
    <PackageTags>starkbank, stark, bank, openbanking, open, banking</PackageTags>
    <Title>StarkBank SDK</Title>
    <Description>SDK to facilitate C# (.NET) integrations with the Stark Bank API</Description>
    <PackOnBuild>true</PackOnBuild>
    <Authors>Stark Bank</Authors>
    <PackageId>starkbank</PackageId>
    <PackageIconUrl>https://dka575ofm4ao0.cloudfront.net/pages-transactional_logos/retina/135476/logo-white-1.png</PackageIconUrl>
    <PackageLicenseUrl>https://opensource.org/licenses/MIT</PackageLicenseUrl>
    <Configurations>Release;Debug</Configurations>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugType></DebugType>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <PackageReference Include="Microsoft.CSharp" Version="4.7.0" />
    <PackageReference Include="starkbank-ecdsa" Version="1.3.1" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Utils\" />
    <Folder Include="User\" />
    <Folder Include="Transaction\" />
    <Folder Include="Boleto\" />
    <Folder Include="Invoice\" />
    <Folder Include="DictKey\" />
    <Folder Include="Transfer\" />
    <Folder Include="Webhook\" />
    <Folder Include="Balance\" />
    <Folder Include="Event\" />
    <Folder Include="BoletoPayment\" />
    <Folder Include="UtilityPayment\" />
    <Folder Include="Settings\" />
    <Folder Include="PaymentRequest\" />
    <Folder Include="BoletoHolmes\" />
    <Folder Include="Deposit\" />
    <Folder Include="BrcodePayment\" />
    <Folder Include="BrcodePreview\" />
    <Folder Include="Workspace\" />
    <Folder Include="Institution\" />
    <Folder Include="TaxPayment\" />
    <Folder Include="DarfPayment\" />
    <Folder Include="PaymentPreview\" />
  </ItemGroup>
  <ItemGroup>
  </ItemGroup>
  <ItemGroup>
    <None Remove="Payment\Boleto\.DS_Store" />
  </ItemGroup>
</Project>
